html,
body
  font-family $os
body
  overflow-x hidden
  bgc $dark
         
.container
  w 100%
  max-width 1573px
  margin 0 auto
  padding 0 180px
.main
  h1:not([class])
    color black
    fontify 96px 700 100px
    margin 63px 0 35px
    text-align center
  h2:not([class])
    color black
    fontify 48px 400 56px
    margin 63px 0 -3px
  h3:not([class]),
  h4:not([class])
    color black
    fontify 36px 400 44px
    margin 63px 0 20px
  img:not([class])
    margin 4px 0
    max-width 100%
  ul:not([class])
    list-style  none
    margin  18px 0
  ul:not([class]) li
    pl 30px
    pos relative
    fontify 18px 400 26px
  ul:not([class]) li + li
    mt 24px
  ul:not([class]) > li:before
    content ""
    pos absolute
    l 0
    t 14px
    size 20px 1px
    bgc $blue
  ol:not([class])
    list-style none
    margin 18px 0 30px
    counter-reset myCounter
  ol:not([class]) li
    pl 30px
    pos relative
    fontify 18px 400 26px
    @media $desktopMedium
      fontify 17px 400 25px
  ol:not([class]) li + li
    mt 16px
  ol:not([class]) > li:before
    counter-increment myCounter
    content counter(myCounter)"."	
    pos absolute
    l 0
    t 0
    w 30px
    pr 9px
    color $blue
    fontify 18px 700 26px
  p:not([class])
    fontify 18px 400 26px
    margin 20px 0
  a:not([class])
    color $blue
    border-bottom 1px solid alpha($blue, .4)
    transition .5s border
    &:hover
      border-color transparent
 
  *:not([class]):not(span):first-child
    mt 0 !important
  *:not([class]):not(span):last-child
    margin-bottom 0 !important

  table
    border 2px solid $blue
    margin 30px 0
    td
      padding 20px 24px
      color $dark
      fontify 15px 400 21px
      border 1px solid #e9f1e6

input::-webkit-input-placeholder
  color alpha(white, .5)
input::-moz-placeholder
  color alpha(white, .5)
input:-ms-input-placeholder
  color alpha(white, .5)
    
div[class\^="table-scroller"]
  overflow auto
  pos relative
  pl 1px
  &:not(:last-child)
    mb 36px
  @media $desktopMax
    w 100%

.text
  max-width 876px
  margin 0 auto
  @media $desktopMax
    max-width 764px
  @media $desktopTablet
    max-width 100%
    
.common
  &__title
    fontify 100px 700 100px
    ml -8px
    &_bigger
      fontify 120px 700 104px
  &__subtitle
    fontify 24px 600 32px
    mt 38px
  &__input
    w 100%
    h 50px
    border 0
    border-bottom 1px solid $dark
    bgc transparent
    color $dark
    font-family $os
    font-size 18px
    transition .7s border
    &.notempty, 
    &:focus
      border-bottom 1px solid $blue
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      opacity .35
      t -7px
      font-size 12px
      line-height 12px
  &__textarea
    w 100%
    h 135px
    overflow hidden
    resize none
    padding 12px 0
    border 0
    border-bottom 1px solid $dark
    bgc transparent
    color $dark
    font-family $os
    fontify 18px 400 26px
    transition .7s border
    & + ^[0]__placeholder
      line-height 22px
    &.notempty, 
    &:focus
      border-bottom 1px solid $blue
    &:focus + ^[0]__placeholder,
    &.notempty + ^[0]__placeholder
      opacity .35
      t -7px
      font-size 12px
      line-height 12px
  &__select
    h 36px
    bgc #fff
    border 2px solid #b6b6b6
    w auto
    cursor pointer
    transition .5s border
    &.warning
      border-color #ed1c24
  &__placeholder
    pos absolute
    z-index -1
    l 0 
    t 0
    color $dark
    fontify 18px 400 48px
    white-space nowrap
    overflow hidden
    max-width 100%
    transition .7s top, .7s line-height, .7s font-size, .7s opacity
  &__col
    pos relative
    z-index 1
    w calc(50% - 24px)
  &__row
    display flex
    justify-content space-between
    pos relative
    w 100%
    & + &
      mt 35px
  &__fileover
    w 100%
    pos relative
    mt 13px
    transition .5s border
    &.warning
      border-color #ed1c24
    & + &
      mt 20px
  &__file
    pos absolute
    w 0
    h 0
    l 0
    t 0
    opacity 0
    &[data-req]:after
      content "*"
      pos absolute
      r -11px
      fontify 23px 400 14px
      t -3px
    &name
      ib()
      pos relative
      l 23px
      mr 23px
      cursor pointer
      color $dark
      fontify 18px 400 18px
      border-bottom 1px dotted $dark
      transition .5s border
      &.choosed
        color white
        text-transform none
        letter-spacing 0
        font-weight 300
      &.choosed,
      &:hover
        border-color transparent
      &:before
        content ""
        pos absolute
        l -23px
        t 1px
        size 16px 15px
        background: url('data:image/png;base64,') no-repeat center
    &delete
      ib()
      visibility hidden
      pos relative
      ml 35px
      pl 22px
      color $dark
      fontify 18px 400 18px
      &.choosed
        visibility visible
        cursor pointer
      &:before
        content ""
        pos absolute
        l 0
        size 15px
        background url('data:image/png;base64,') no-repeat center

.btn
  font-family $os
  border 1px solid $blue
  padding 0 24px
  fontify 18px 600 44px
  bgc $blue
  color white
  transition .7s background
  &:hover
    background transparent

.fakecheckbox
  ib()
  pos relative
  &__label
    ib()
    color alpha(white, .5)
    fontify 14px 300 20px
    pos relative
    pl 30px
    pt 9px
    &:before 
      content ""
      pos absolute
      l 0px
      t 8px
      size 16px
      border 2px solid alpha(white, .5)
      border-radius 4px
      cursor pointer
      transition .5s border
    &:after
      content ""
      pos absolute
      size 10px 7px
      background url('data:image/png;base64,') no-repeat center
      l 5px
      t 15px
      opacity 0
      cursor pointer
      transition .5s opacity
    a
      color inherit
      border-bottom 1px solid alpha(white, .2)
      transition .7s border
      &:hover
        border-color transparent
  &__truecheck
    pos absolute
    l 0
    t 0
    w 0
    h 0
    border 0
    opacity 0
    &:checked + ^[0]__label:after
      opacity 1
    &.warning + ^[0]__label:before 
      border-color #ff0f37

.breadcrumbs
  font-family $sans
  text-align center
  pl 18px
  mb 70px
  color black
  @media $LandscapeMin
    mb 40px
  &__href
    color black
    fontify 11px 700 14px
    text-transform uppercase
    letter-spacing .12em
    border-bottom 1px solid alpha(black, .25)
    transition .5s border
    &:hover
      border-color transparent
  &__delim
    fontify 11px 700 14px
    margin 0 5px

h1:not([class]) + .breadcrumbs
  mt -6px
      
input.warning,
textarea.warning
  border-color #ff0f37

.hidden-block
  display none