.news
  &__elem
    &-bottom
      df(flex-start, space-between)
      mt 48px
    &-date
      fontify 14px 600 20px
    &-prev,
    &-next
      pos fixed
      t 50%
      size 33px 31px
      background url('data:image/png;base64,') no-repeat center
      mt -15px
      transition .7s background
      @media $desktopMin
        dn()
      &:hover
        background url('data:image/png;base64,') no-repeat center
    &-prev
      l 30px
      @media $desktopMax
        l 10px
    &-next
      r 30px
      transform scaleX(-1)
      @media $desktopMax
        r 10px
  &__socials
    df()
    mt 4px
    &-href
      transition .7s opacity
      &:hover
        opacity .7
      & + &
        ml 28px
      &[data-type="fb"]
        size 13px
        background url('data:image/png;base64,') no-repeat center
      &[data-type="tw"]
        size 15px 12px
        background url('data:image/png;base64,') no-repeat center